#include "header.h"
#include <stdio.h>
#include <stdlib.h>
/**
 *str_concat - concat strings
 *@s1: string to copy.
 *@s2: dest.
 *Return: char.
 */
char *getpath(char param[], char *envp[])
{
        char delim1[] = ":";
        char delim2[] = "=";
        char *envpath = NULL;
        char *listpaths = NULL;
        int i;
        char *strtmp;

	for (i = 0; envp[i] != NULL; i++)
	{
		envpath = strtok( envp[i], delim2 );
		while( envpath != NULL ) {
			if(_strcmp(envpath,param) == 0)
			{
				envpath = strtok( NULL, delim2);
				printf("ENV PATH FOUND: %s", envpath);
				return(envpath);
			}
			envpath = strtok( NULL, delim2);
		}
	}
	return(NULL);
}
